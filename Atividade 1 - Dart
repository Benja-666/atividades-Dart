void main(){
var nomes = 'Deide Costa';

print(nome);

var nomeDiferente = 'Deide Costa';

if(nomes.contains(nomeDiferente)){
  print('É a mesma pessoa');
}else{
  print('Os nomes não conferem');
}

}
