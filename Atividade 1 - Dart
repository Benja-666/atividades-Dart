void main() {
var valorDoIngresso = 33;
var pessoas = 4;
  
int totalSomado = valorDoIgresso * pessoas;    
 
print("Valor do Ingresso: $valorDoIngresso R\$");
print("Quantidade de pessoas:  $pessoas ");
print("Valor dos ingressos: $valorDoIngresso R\$");

}

___________________________________________________________________________________

void main(){
int base1 = 20;
int base2 = 10;
int altura = 5;

double area = ((base1 = base2) * altura) /2;

print('Calcula da area do trapézio é: $area');

}

__________________________________________________________________________________

void main(){
double altura = 70;
double largura = 90;

double area = altura * largura;

print('calculo da Area do quadrado é: $area');

}

