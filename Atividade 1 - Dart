Bernardo Corra de Moura - Conoas.

void main(){
var nome = 'Bernardo';
var sobrenome = 'Correa';
var idade = '17 anos';
var endereco = 'Rua uruguai 588';

print('Nome: $nome $sobrenome');
print('dade: $idade');
print('Endereço: $endereco');
}

