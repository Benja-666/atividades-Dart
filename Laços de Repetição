void main() {

int tabuada = 5;
  
for(int i = 1; i <= 10; i++) {
 int resultado = tabuada * i;
  print("$tabuada X $i: $resultado");
  }
}(atividade 6)

____________________________________________________________

void main() {
  
  int calculo = 5;
  for(int i = calculo -1; i > 0; i--) {
    calculo *= i;
    print("X$calculo");
  }
}(atividade 8)

