void main() {

int numero = 0;
  
  for(int x = 0; 1 <= 10; x++)
  {
    print("Valores" + x.toString());
    
  numero = x;
      
  if(x >= numero);
    {
 }
}
print("O maior numero é:"+ numero.toString());
  }

